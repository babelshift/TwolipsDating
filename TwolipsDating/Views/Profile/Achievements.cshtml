@model TwolipsDating.ViewModels.AchievementManagerViewModel

@{
    ViewBag.Title = "Your achievements";
}

<div class="col-xs-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            <div class="panel-title">
                <i class="fa fa-trophy"></i> Achievements
            </div>
        </div>
        <div class="panel-body">
            @Html.Partial("_SignUpIncentivePartial")
            @foreach (var achievement in Model.Achievements)
            {
                @Html.Partial("_AchievementOverviewPartial", achievement)
            }
        </div>
    </div>
</div>